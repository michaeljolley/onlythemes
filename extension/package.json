{
  "name": "onlythemes",
  "displayName": "Only Themes",
  "version": "0.0.1",
  "description": "Find that special theme you've been looking for.",
  "main": "./dist/extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/buildersclub/onlythemes"
  },
  "keywords": [
    "themes"
  ],
  "author": {
    "name": "Michael Jolley",
    "email": "mike@baldbeardedbuilder.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buildersclub/onlythemes/issues"
  },
  "publisher": "michaeljolley",
  "icon": "resources/logo.png",
  "preview": false,
  "engines": {
    "vscode": "^1.53.0"
  },
  "galleryBanner": {
    "color": "#373436",
    "theme": "dark"
  },
  "extensionKind": [
    "workspace"
  ],
  "homepage": "https://baldbeardedbuilder.com",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:onlyThemesView"
  ],
  "contributes": {
    "configuration": {
      "title": "OnlyThemes",
      "properties": {
        "onlythemes.telemetry.enabled": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Specifies whether to enable OnlyThemes telemetry (even if enabled still abides by the overall `#telemetry.enableTelemetry#` setting)",
          "scope": "window"
        }
      }
    },
    "commands": [
      {
        "category": "OnlyThemes",
        "command": "onlyThemes.getThemeSuggestion",
        "title": "Get Theme Suggestion"
      }
    ],
    "views": {
      "onlyThemes": [
        {
          "type": "webview",
          "id": "onlyThemesView",
          "name": "Only Themes"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "./resources/icon_activitybar.svg",
          "id": "onlyThemes",
          "title": "Only Themes"
        }
      ]
    }
  },
  "scripts": {
    "package": "npx vsce package -o release.vsix",
    "vscode:prepublish": "webpack --mode production",
    "compile": "tsc -p ./ && webpack",
    "lint": "eslint --ext .ts .",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "webpack": "webpack --mode production",
    "test": "node ./out/tests/runTest.js"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "os-name": "^4.0.0",
    "public-ip": "^4.0.3",
    "universal-analytics": "^0.4.23",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.8",
    "@types/sinon": "^9.0.9",
    "@types/universal-analytics": "^0.4.4",
    "@types/uuid": "^8.3.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^4.16.0",
    "@typescript-eslint/parser": "^4.16.0",
    "chai": "^4.3.3",
    "copy-webpack-plugin": "^7.0.0",
    "eslint": "^7.21.0",
    "glob": "^7.1.4",
    "mocha": "^8.2.1",
    "sinon": "^9.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.2.2",
    "vscode-codicons": "0.0.14",
    "vscode-test": "^1.4.1",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.6.0"
  }
}