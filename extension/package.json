{
  "name": "onlythemes",
  "displayName": "Only Themes",
  "version": "1.1.4",
  "description": "Find that special theme you've been looking for.",
  "main": "./dist/extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/builders-club/onlythemes"
  },
  "keywords": [
    "themes"
  ],
  "author": {
    "name": "Michael Jolley",
    "email": "mike@baldbeardedbuilder.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/builders-club/onlythemes/issues"
  },
  "publisher": "builders-club",
  "icon": "resources/logo.png",
  "preview": false,
  "engines": {
    "vscode": "^1.53.0"
  },
  "galleryBanner": {
    "color": "#373436",
    "theme": "dark"
  },
  "extensionKind": [
    "workspace"
  ],
  "homepage": "https://baldbeardedbuilder.com",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:onlyThemesView",
    "onView:thumbnailView",
    "onCommand:onlythemes.forgetMe"
  ],
  "contributes": {
    "configuration": {
      "title": "OnlyThemes",
      "properties": {
        "onlythemes.telemetry.enabled": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Specifies whether to enable OnlyThemes telemetry (even if enabled still abides by the overall `#telemetry.enableTelemetry#` setting)",
          "scope": "window"
        }
      }
    },
    "commands": [
      {
        "command": "onlythemes.forgetMe",
        "title": "Forget My Ratings",
        "category": "OnlyThemes"
      }
    ],
    "views": {
      "onlyThemes": [
        {
          "type": "webview",
          "id": "onlyThemesView",
          "name": "Only Themes"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "./resources/icon_activitybar.svg",
          "id": "onlyThemes",
          "title": "Only Themes"
        }
      ]
    }
  },
  "scripts": {
    "package": "npx vsce package -o release.vsix",
    "vscode:prepublish": "webpack --mode production",
    "compile": "tsc -p ./ && webpack",
    "lint": "eslint --ext .ts .",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "webpack": "webpack --mode production",
    "test": "node ./out/tests/runTest.js"
  },
  "dependencies": {
    "node-fetch": "^3.2.10",
    "os-name": "^5.0.1",
    "public-ip": "^6.0.0",
    "universal-analytics": "^0.5.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/sinon": "^10.0.11",
    "@types/universal-analytics": "^0.4.5",
    "@types/uuid": "^8.3.4",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "chai": "^4.3.6",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "sinon": "^14.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "vscode-codicons": "0.0.14",
    "vscode-test": "^1.4.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}